{% parse_json object %}
  {
    "id":          {{ object.id | json }},
    "owner":       {{ owner | json }},
    "name":        {{ object.name | json }},
    "tags":        {{ object.tags | downcase | split: ',' | uniq }},
    "description": {{ object.description | default: "" | json }},
    "category":    {{ object.category | default: "" | json }},
    "price":       {{ object.price | plus: 0 | times: 100 | to_positive_integer: 1 }},
    "uuid":        {{ object.uuid | json }},
    "title":        {{ object.title | json }},
    "runtime":     {{ object.runtime | json }},
    "director":    {{ object.director | json }},
    "year":        {{ object.year | json }}


  }
{% endparse_json %}

{% liquid
  return_rc object
%}
